<% provide(:title, "New Category") %>  
<div class="row">
  <div class="col-md-6 col-md-offset-3">    
    <h1>New Category</h1>
    <%= form_for [:admin, @category] do |f| %>      
      <%= render "fields",f: f %>        
      <%= f.submit "Create Category", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>