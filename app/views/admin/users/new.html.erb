<% provide(:title, "New User") %>  
<div class="row">
  <div class="col-md-6 col-md-offset-3">    
    <h1>New User</h1>
    <%= form_for [:admin, @user] do |f| %>      
      <%= render "fields",f: f %>        
      <%= f.submit "Create User account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>