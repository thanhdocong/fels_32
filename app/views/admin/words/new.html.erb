<% provide(:title, "New Word") %>  
<div class="row">
  <div class="col-md-6 col-md-offset-3">    
    <h1>New Word</h1>    
    <%= form_for [:admin, @word] do |f| %>              
      <%= render "fields", f: f %>        
      <%= f.submit "Create User Word", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>  